{
  "name": "nazboyko/sorted-linked-list",
  "description": "A type-safe, sorted linked list for int or string values.",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Nazar Boyko",
      "email": "boyko.nazar@gmail.com"
    }
  ],
  "require": {
    "php": "^8.2 || ^8.3 || ^8.4"
  },
  "require-dev": {
    "phpunit/phpunit": "^10.5 || ^11.0",
    "phpstan/phpstan": "^1.11",
    "vimeo/psalm": "^5.26"
  },
  "autoload": {
    "psr-4": {
      "NazBoyko\\SortedLinkedList\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "NazBoyko\\SortedLinkedList\\Tests\\": "tests/"
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}
